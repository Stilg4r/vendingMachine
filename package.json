{
  "name": "robespierre",
  "version": "0.1.0",
  "description": "Headless CMS",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --verbose",
    "tdd": "npx nodemon -w src -w tests -w __mocks__ --exec npm test",
    "prepare": "husky install",
    "lint": "npx eslint ./src --fix --fix-type suggestion --cache ",
    "build": "babel src -d dist",
    "prestart": "npm run build",
    "start": "NODE_ENV=production node dist/index.js",
    "babel": "npx babel-node --presets=@babel/preset-env --",
    "dev": "NODE_ENV=development nodemon -w ./src --exec npm run babel ./src/index.js",
    "lint:dev": "npm run lint && npm run dev"
  },
  "keywords": [
    "CRM"
  ],
  "author": "@Stilg4r",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "morgan": "^1.10.0",
    "sqlite3": "^5.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "babel-jest": "^27.5.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "hygen": "^6.2.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1"
  }
}
